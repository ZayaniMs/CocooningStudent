<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AccueilBundle\Entity\Annonces" table="annonces">
    <indexes>
      <index name="fk_Annonces_Adresse_idx" columns="Adresse_idAdresse"/>
    </indexes>
    <id name="idannonces" type="integer" column="idannonces">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rue" type="string" column="rue" length="45" nullable="true"/>
    <field name="numero" type="string" column="numero" length="45" nullable="true"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
    <field name="prix" type="string" column="prix" length="45" nullable="true"/>
    <field name="date" type="datetime" column="date" nullable="true"/>
    <field name="visible" type="string" column="visible" length="45" nullable="true"/>
    <many-to-one field="adresseadresse" target-entity="Adresse">
      <join-columns>
        <join-column name="Adresse_idAdresse" referenced-column-name="idAdresse"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
