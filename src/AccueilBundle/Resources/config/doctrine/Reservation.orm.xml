<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AccueilBundle\Entity\Reservation" table="reservation">
    <indexes>
      <index name="fk_Reservation_Annonces1_idx" columns="Annonces_idannonces"/>
      <index name="fk_Reservation_Users1_idx" columns="Users_idUsers"/>
    </indexes>
    <id name="idreservation" type="integer" column="idReservation">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="debut" type="datetime" column="debut" nullable="true"/>
    <field name="statut" type="string" column="statut" length="45" nullable="true"/>
    <field name="payer" type="string" column="payer" length="45" nullable="true"/>
    <many-to-one field="usersusers" target-entity="Users">
      <join-columns>
        <join-column name="Users_idUsers" referenced-column-name="idUsers"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="annoncesannonces" target-entity="Annonces">
      <join-columns>
        <join-column name="Annonces_idannonces" referenced-column-name="idannonces"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
