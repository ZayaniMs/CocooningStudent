<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AccueilBundle\Entity\Users" table="users">
    <indexes>
      <index name="fk_Users_Adresse1_idx" columns="Adresse_idAdresse"/>
    </indexes>
    <id name="idusers" type="integer" column="idUsers">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" nullable="true"/>
    <field name="prenom" type="string" column="Prenom" length="45" nullable="true"/>
    <field name="mdp" type="string" column="mdp" length="45" nullable="true"/>
    <field name="rue" type="string" column="rue" length="45" nullable="true"/>
    <field name="numero" type="string" column="numero" length="45" nullable="true"/>
    <field name="email" type="string" column="email" length="45" nullable="true"/>
    <many-to-one field="adresseadresse" target-entity="Adresse">
      <join-columns>
        <join-column name="Adresse_idAdresse" referenced-column-name="idAdresse"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
